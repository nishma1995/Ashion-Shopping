    <script>

        $(document).ready(function () {

            $('#clear-search').click(function () {
                $('#search-input').val('')
            })

            $('.search-model-form').on('submit', function (event) {
                event.preventDefault();

                const searchText = $('#search-input').val();

                // Construct the URL with the search query as a parameter
                const searchUrl = `/search?search=${encodeURIComponent(searchText)}`;

                // Redirect to the search URL
                window.location.href = searchUrl;
            });
          

        function addToWishlist(productId, price) {
            event.preventDefault()

            $.ajax({
                url: '/addToWishlist',
                type: 'POST',
                contentType: 'application/json',
                data: JSON.stringify({ productId, price }),
                success: function (response) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: 'Product added to wishlist!',
                        timer: 4000, // Duration in milliseconds
                        toast: true,
                        position: 'top', // Toast position
                        showConfirmButton: false // Hide confirmation button
                    });
                },
                error: function (xhr, status, error) {
                    if (xhr.responseJSON && xhr.responseJSON.error) {
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: xhr.responseJSON.error,
                            timer: 4000,
                            toast: true,
                            position: 'top',
                            showConfirmButton: false
                        });
                    } else {
                        console.error('Error adding product to wishlist:', error);
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'Error adding product to wishlist. Please try again.',
                            timer: 4000,
                            toast: true,
                            position: 'top',
                            showConfirmButton: false
                        });
                    }
                }
            });
        }
        })

    </script>

